{
  "name": "rune-world",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "npm run lint && tsc && vite build",
    "upload": "npm run build && npx rune@latest upload",
    "lint": "eslint src --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,html}\""
  },
  "dependencies": {
    "nipplejs": "^0.10.2",
    "propel-js": "^1.0.26",
    "rune-sdk": "^4.25.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.8.0",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.2.11",
    "vite-plugin-qrcode": "^0.2.2"
  }
}
